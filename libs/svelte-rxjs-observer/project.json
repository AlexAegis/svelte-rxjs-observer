{
	"name": "svelte-rxjs-observer",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/svelte-rxjs-observer/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/svelte-rxjs-observer/**/*.{ts,svelte,spec.ts}"]
			}
		},
		"check": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"command": "svelte-check",
				"cwd": "libs/svelte-rxjs-observer"
			}
		},
		"supersimplebuild": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/svelte-rxjs-observer",
				"main": "libs/svelte-rxjs-observer/src/index.ts",
				"tsConfig": "libs/svelte-rxjs-observer/tsconfig.lib.json",
				"assets": [
					"libs/svelte-rxjs-observer/*.md",
					{
						"glob": "*.svelte",
						"input": "./libs/svelte-rxjs-observer/src/",
						"output": "./src"
					}
				]
			}
		},
		"build": {
			"executor": "@nxext/vite:package",
			"outputs": ["{options.outputPath}"],
			"options": {
				"entryFile": "src/index.ts",
				"frameworkConfigFile": "@nxext/svelte/plugins/vite",
				"outputPath": "dist/libs/svelte-rxjs-observer",
				"assets": [
					{
						"glob": "/*",
						"input": "./public/**",
						"output": "./"
					}
				],
				"tsConfig": "libs/svelte-rxjs-observer/tsconfig.lib.json"
			},
			"configurations": {
				"production": {}
			}
		},
		"test": {
			"executor": "@nxext/vitest:vitest",
			"options": {
				"command": "run --coverage"
			}
		}
	}
}
