{
	"name": "svelte-rxjs-observer",
	"displayName": "svelte-rxjs-observer",
	"description": "A svelte component that makes observing obsevables easier",
	"version": "0.2.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/svelte-rxjs-observer#readme",
	"repository": "github:AlexAegis/svelte-rxjs-observer",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/svelte-rxjs-observer/issues"
	},
	"keywords": [
		"sveltejs",
		"rxjs",
		"reactive",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"workspaces": [
		"libs/svelte-rxjs-observer",
		"apps/example"
	],
	"scripts": {
		"serve:svelte": "nx run example:serve",
		"format": "prettier --write .",
		"lint": "nx run-many --parallel 4 --target lint",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check .",
		"lint:style": "stylecheck",
		"test:jest": "jest --passWithNoTests",
		"ncu": "ncu -u",
		"clean": "rimraf node_modules package-lock.json",
		"ng": "nx",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"postinstall": "husky install && node ./decorate-angular-cli.js",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"start": "nx serve",
		"build": "nx run-many --parallel 4 --target build --configuration production",
		"build:lib": "cd libs/svelte-rxjs-observer && svelte-package && rm -rf ../../dist/libs/svelte-rxjs-observer && mkdir -p ../../dist/libs/svelte-rxjs-observer && mv package/* ../../dist/libs/svelte-rxjs-observer",
		"build:example": "nx run example:build --configuration production",
		"test": "nx run-many --parallel 4 --target test --coverage"
	},
	"dependencies": {
		"@nxext/svelte": "^14.0.2",
		"@nxext/vite": "15.0.0-alpha.5",
		"rxjs": "7.5.7",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "~17.1.2",
		"@commitlint/config-conventional": "~17.1.0",
		"@nrwl/cli": "15.0.2",
		"@nrwl/cypress": "15.0.2",
		"@nrwl/eslint-plugin-nx": "15.0.2",
		"@nrwl/js": "15.0.2",
		"@nrwl/linter": "15.0.2",
		"@nrwl/workspace": "15.0.2",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "1.0.0-next.525",
		"@sveltejs/package": "^1.0.0-next.5",
		"@sveltejs/vite-plugin-svelte": "^1.1.0",
		"@testing-library/svelte": "^3.2.2",
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "18.11.5",
		"@typescript-eslint/eslint-plugin": "5.41.0",
		"@typescript-eslint/parser": "5.41.0",
		"@vitest/coverage-c8": "^0.24.3",
		"c8": "^7.12.0",
		"carbon-components-svelte": "^0.70.12",
		"carbon-icons-svelte": "^11.4.0",
		"cypress": "^10.11.0",
		"dotenv": "~16.0.3",
		"eslint": "8.26.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~4.2.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"jsdom": "^20.0.1",
		"lint-staged": "~13.0.3",
		"markdownlint": "^0.26.2",
		"markdownlint-cli": "^0.32.2",
		"npm-check-updates": "~16.3.15",
		"nx": "15.0.2",
		"pinst": "^3.0.0",
		"prettier": "2.7.1",
		"regenerator-runtime": "^0.13.10",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.21",
		"standard-version": "^9.5.0",
		"stylelint": "^14.14.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-prettier": "^2.0.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.7",
		"ts-node": "10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "4.8.4",
		"vite-plugin-dts": "^1.6.6",
		"vite-plugin-static-copy": "^0.11.0",
		"vitest": "^0.24.3",
		"webpack-bundle-analyzer": "~4.6.1"
	}
}
