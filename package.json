{
	"name": "svelte-rxjs-observer",
	"displayName": "svelte-rxjs-observer",
	"description": "A svelte component that makes observing obsevables easier",
	"version": "0.3.0",
	"license": "MIT",
	"private": true,
	"keywords": [
		"javascript",
		"js",
		"managed-by-autotool",
		"reactive",
		"rxjs",
		"sveltejs",
		"ts",
		"typescript"
	],
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/svelte-rxjs-observer#readme",
	"repository": "github:AlexAegis/svelte-rxjs-observer",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/svelte-rxjs-observer/issues"
	},
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=18.10.0",
		"npm": ">=8.0.0",
		"pnpm": ">=8.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build-lib_ build-app_ --concurrency 16 --cache-dir .cache/turbo",
		"build-app": "turbo run build-app_ --concurrency 16 --cache-dir .cache/turbo",
		"build-lib": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo",
		"lint": "turbo run lint_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:format_": "prettier --no-plugin-search --cache-location .cache/prettier --ignore-path .config/workspace-only.prettierignore --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:md_": "remark --frail --no-stdout --silently-ignore --ignore-pattern packages --ignore-pattern libs --ignore-pattern apps .",
		"lint:rmd": "turbo run lint:rmd",
		"lint:style": "turbo run lint:style_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:html": "turbo run lint:style:html_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo",
		"publint": "turbo run publint_ --concurrency 16 --cache-dir .cache/turbo",
		"all": "turbo run all_ --concurrency 16 --cache-dir .cache/turbo",
		"ce": "turbo run ce_ --concurrency 6 --cache-dir .cache/turbo",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo",
		"format_": "prettier --no-plugin-search --cache-location .cache/prettier --ignore-path .config/workspace-only.prettierignore --write .",
		"merge-coverage": "merge-workspace-lcov-reports && lcov-viewer lcov -o ./coverage ./coverage/lcov.info",
		"ncu": "ncu --deep --peer --upgrade && pnpm up",
		"nuke": "nuke",
		"release": "commit-and-tag-version --sign --commit-all",
		"release:major": "commit-and-tag-version --sign --commit-all --release-as major",
		"release:minor": "commit-and-tag-version --sign --commit-all --release-as minor",
		"release:patch": "commit-and-tag-version --sign --commit-all --release-as patch",
		"setup-all": "setup-prettier && setup-editorconfig && setup-husky && setup-git && setup-commitlint && setup-eslint && setup-github && setup-ts && setup-remark && setup-standard-version && setup-stylelint && setup-depcheck && setup-turbo && setup-vite && setup-vitest && setup-workspace",
		"start": "turbo run start --parallel",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo",
		"typedoc": "turbo run typedoc_ --concurrency 16 --cache-dir .cache/turbo",
		"typedoc_": "typedoc"
	},
	"devDependencies": {
		"@alexaegis/autotool-plugin-commitlint": "^0.3.1",
		"@alexaegis/autotool-plugin-depcheck": "^0.3.1",
		"@alexaegis/autotool-plugin-editorconfig": "^0.3.1",
		"@alexaegis/autotool-plugin-eslint": "^0.3.1",
		"@alexaegis/autotool-plugin-git": "^0.3.1",
		"@alexaegis/autotool-plugin-github": "^0.3.1",
		"@alexaegis/autotool-plugin-husky": "^0.3.1",
		"@alexaegis/autotool-plugin-prettier": "^0.3.1",
		"@alexaegis/autotool-plugin-publint": "^0.3.1",
		"@alexaegis/autotool-plugin-remark": "^0.3.1",
		"@alexaegis/autotool-plugin-standard-version": "^0.3.1",
		"@alexaegis/autotool-plugin-stylelint": "^0.3.1",
		"@alexaegis/autotool-plugin-svelte": "^0.3.1",
		"@alexaegis/autotool-plugin-ts": "^0.3.1",
		"@alexaegis/autotool-plugin-turbo": "^0.3.1",
		"@alexaegis/autotool-plugin-typedoc": "^0.3.1",
		"@alexaegis/autotool-plugin-vite": "^0.3.1",
		"@alexaegis/autotool-plugin-vitest": "^0.3.1",
		"@alexaegis/autotool-plugin-vscode": "^0.3.1",
		"@alexaegis/autotool-plugin-workspace": "^0.3.1",
		"@alexaegis/coverage-tools": "^0.2.1",
		"@alexaegis/eslint-config-core": "^0.3.1",
		"@alexaegis/nuke": "^0.3.1",
		"@alexaegis/stylelint-config": "^0.3.1",
		"@alexaegis/ts": "^0.3.1",
		"@lcov-viewer/cli": "^1.3.0",
		"@types/eslint": "^8.40.0",
		"@types/node": "20.2.5",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"@vitest/coverage-c8": "^0.31.2",
		"autotool": "^0.0.5",
		"commit-and-tag-version": "^11.2.1",
		"commitlint": "^17.6.5",
		"depcheck": "^1.4.3",
		"eslint": "^8.41.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"npm-check-updates": "^16.10.12",
		"prettier": "^2.8.8",
		"remark": "^14.0.3",
		"remark-cli": "^11.0.0",
		"stylelint": "^15.6.2",
		"svelte-check": "^3.4.3",
		"ts-node": "^10.9.1",
		"turbo": "^1.10.0",
		"typedoc": "^0.24.7",
		"typescript": "^5.0.4",
		"vitest": "^0.31.2"
	}
}
